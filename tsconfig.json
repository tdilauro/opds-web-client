{
  "version": "1.7.5",
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "lib": ["es2015", "dom"],
    "outDir": "lib",
    "rootDir": "src",
    "jsx": "react",
    "types": ["mocha", "node"],
    "declaration": true,
    "strictNullChecks": true
  },
  "exclude": ["node_modules", "lib", "dist"],
  "typedocOptions": {
    "mode": "file",
    "out": "docs"
  }
}
